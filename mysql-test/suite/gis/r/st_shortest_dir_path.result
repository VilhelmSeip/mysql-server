# ---------------------
# |[0] creating tables|
# ---------------------
CREATE TABLE edges (
id INT PRIMARY KEY,
from_id INT,
to_id INT,
cost DOUBLE,
type ENUM("undefined", "car", "bike", "pedestrian", "spatial")
);
