DELETE FROM edges;
CREATE TABLE points (
id INT PRIMARY KEY,
point POINT NOT NULL SRID 0
);
ALTER TABLE edges
ADD CONSTRAINT point_ref FOREIGN KEY (to_id) REFERENCES points(id);
